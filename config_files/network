
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'
	option metric '1'
	option multipath 'off'
	option defaultroute '0'
	option peerdns '0'

config globals 'globals'
	option ula_prefix 'fdb6:eb9b:56c6::/48'
	option multipath 'enable'
	option mptcp_fullmesh_create_on_err '1'
	option mptcp_checksum '0'
	option mptcp_path_manager 'fullmesh'
	option mptcp_debug '0'
	option mptcp_syn_retries '10'
	option congestion 'cubic'
	option mptcp_fullmesh_num_subflows '8'
	option mptcp_ndiffports_num_subflows '4'
	option mptcp_scheduler 'default'

config interface 'lan'
	option type 'bridge'
	option ifname 'eth0.10'
	option proto 'static'
	option ipaddr '192.168.77.1'
	option netmask '255.255.255.0'
	option ip6assign '60'
	option metric '2'
	option multipath 'off'
	option mtu '9000'
	option txqueuelen '60000'

config device 'lan_dev'
	option name 'eth0.10'

